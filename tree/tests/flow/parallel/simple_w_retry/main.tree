import "std::actions"

cond fail_before_tick();
cond increment(key:string,default:num);

root main repeat(2) parallel {
    r_fallback {
        fail_before_tick()
        running()
    }
    r_fallback {
        r_sequence {
            fail_before_tick()
            store_tick("tick1")
        }
        running()
    }
    store_tick("tick2")
}
