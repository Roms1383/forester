import "std::actions"

cond fail_before_tick();
cond incr(key:string,default:num);

root main repeat(3) parallel {
    r_fallback {
        fail_before_tick()
        running()
    }
    r_fallback {
        r_sequence {
            fail_before_tick()
            incr("t1",0)
        }
        running()
    }
    incr("t2",0)
}
